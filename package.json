{
  "name": "aira-js",
  "version": "0.2.0",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vol4tim/aira-js.git"
  },
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "webpack --mode development --output-library Aira --output-library-target umd --output-filename aira.js",
    "build:umd:min": "webpack --mode production --output-library Aira --output-library-target umd --output-filename aira.min.js",
    "build": "npm run build:commonjs && npm run build:es",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "nodemon": "^1.17.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "base-58": "^0.0.1",
    "base-64": "^0.1.0",
    "bluebird": "^3.5.1",
    "eth-lib": "^0.2.8",
    "lodash": "^4.17.5",
    "web3-utils": "^1.0.0-beta.33"
  }
}
